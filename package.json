{
  "name": "alfa-id",
  "version": "1.0.2",
  "description": "Generate short random IDs (optionally unique) for JS & TS",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "npm run build"
  },
  "keywords": ["id", "generator", "shortid", "uuid", "typescript", "random", "unique"],
  "author": "Mohit Joe R",
  "license": "MIT"
}